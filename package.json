{
  "name": "swiftcloudapi",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "test": "jest",
    "start": "ts-node src/app.ts",
    "tsc": "tsc src/app.ts"
  },
  "repository": "",
  "author": "Matt Blair",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.14",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0",
    "jest": "^26.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "mongodb-memory-server": "^6.6.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "latest"
    }
  }
}
